{
  "name": "@simplylinn/tonematrix",
  "version": "1.0.0-dev1",
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "unpkg": "dist/bundle.js",
  "types": "dist/index.d.ts",
  "browser": true,
  "scripts": {
    "watch": "webpack --watch",
    "build": "tsc && webpack",
    "dev:watch": "cross-env NODE_ENV=development webpack --watch",
    "dev:build": "cross-env NODE_ENV=development tsc && cross-env NODE_ENV=development webpack",
    "prod:watch": "cross-env NODE_ENV=production webpack --watch",
    "prod:build": "cross-env NODE_ENV=production tsc && cross-env NODE_ENV=production webpack",
    "prepack": "rimraf dist docs && cross-env NODE_ENV=production tsc && cross-env NODE_ENV=production webpack && typedoc",
    "docs": "typedoc"
  },
  "repository": "github:SimplyLinn/ToneMatrixRedux",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "files": [
    "dist/**/*",
    "docs/**/*",
    "AUTHORS"
  ],
  "directories": {
    "lib": "dist"
  },
  "dependencies": {
    "eventemitter3": "^4.0.4",
    "tone": "^14.7.39"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^3.0.2",
    "@typescript-eslint/parser": "^3.0.2",
    "cross-env": "^7.0.2",
    "eslint": "^7.1.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.20.2",
    "rimraf": "^3.0.2",
    "ts-loader": "^8.0.2",
    "typedoc": "^0.17.0-3",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12"
  }
}
